pico-8 cartridge // http://www.pico-8.com
version 18
__lua__

function _init()
    tackled = false
    loggingEnabled = true
    debugInfo = nil
    fieldPos = 40
    playerY = 100
    playerX = 20
    players = {}
    for num = 1, 5 do
        players[num] = createPlayer(playerX*num, playerY)
        printh(players[num])
    end
    enemies = {}
    for num = 1, 4 do
        enemies[num] = createPlayer(playerX*num, playerY - 80)
    end
    ball = {}
    ball.owner = 1
    ball.pos = {}
    ball.frame = 32
    players[1].ball = true
    ball.direction = 0
    ball.speed = 0.5
end

function createPlayer(x, y)
    player = {}
    player.frame = 1
    player.mode = 1
    player.pos = {}
    player.pos.x = x
    player.pos.y = y
    player.ball = false
    return player
end

function _update()
    handleInput()
    updateField()
    updateBall()
    updatePlayers()
    updateEnemies()
    checkForTackle()
end

function handleInput()
    if (ball.owner != nil) then
        if (btn(4)) then throwPass(-1)
        elseif (btn(5)) then throwPass(1) end
    end
end

function throwPass(direction)
    ball.direction = direction
    holdersPos = players[ball.owner].pos
    ball.pos.x = holdersPos.x + (5*direction) 
    ball.pos.y = holdersPos.y
    players[ball.owner].ball = false
    ball.owner = nil
end

function updateBall()
    if (ball.owner == nil) then
        ball.pos.x += (ball.speed * ball.direction)
        ballPos = ball.pos.x
        for index = 1, 5 do
            player = players[index]
            lb = player.pos.x - 1
            rb = player.pos.x + 1
            if (ballPos > lb and ballPos < rb) then
                ball.direction = 0
                players[index].ball = true
                ball.owner = index
            end
        end
    end
end


function updatePlayers()
    for num = 1, 5 do
        player = players[num]
        if player.frame == 6 then
            player.mode = 0
        elseif player.frame == 0 then
            player.mode = 1
        end
        if player.mode == 1 then
            player.frame = player.frame + 1
        elseif player.mode == 0 then
            player.frame = player.frame -1
        end
    end
end

function updateEnemies()
    for num = 1, 4 do
        player = enemies[num]
        player.pos.y += 2
        -- player.pos.x += 0.5
        if player.frame == 6 then
            player.mode = 0
        elseif player.frame == 0 then
            player.mode = 1
        end
        if player.mode == 1 then
            player.frame = player.frame + 1
        elseif player.mode == 0 then
            player.frame = player.frame -1
        end
    end
end

function checkForTackle()
    if ball.owner != nil then
        player = players[ball.owner]
        for enemyIdx = 1, 4 do
            enemy = enemies[enemyIdx].pos
            lb = player.pos.x - 10
            rb = player.pos.x + 10
            top = player.pos.y - 10
            bot = player.pos.y + 10
            if (enemy.x > lb and enemy.x < rb and enemy.y > top and enemy.y < bot) then
                log(enemyIdx)
                tackled = true
            end
        end
    end
end

function updateField()
    if fieldPos <= 0 then
        fieldPos = 40
    end
    fieldPos -= 0.1
end

function log(info)
    if (loggingEnabled == true) then
        debugInfo = info
    end
end

function _draw()
    -- cls(3)
    map(0,fieldPos,0,0,120,120)
    for num = 1, 5 do
        player = players[num]
        if player.ball == false then
            spr(player.frame, player.pos.x, player.pos.y)
        elseif player.ball == true then
            spr(player.frame+16, player.pos.x, player.pos.y)
        end
    end
    for num = 1, 4 do
        player = enemies[num]
        spr(player.frame+39, player.pos.x, player.pos.y, 1, 1, false, true)
    end
    if (ball.owner == nil) then
        spr(ball.frame, ball.pos.x, ball.pos.y)
    end
    if(debugInfo != nil) then
        print(debugInfo)
    end
end


